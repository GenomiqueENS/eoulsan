<dataformat>
  <name>genome_fasta</name>
  <description>genome fasta format</description>
  <prefix>genome</prefix>
  <onefileperanalysis>true</onefileperanalysis>
  <designmetadatakey>GenomeFile</designmetadatakey>
  <generator>fr.ens.biologie.genomique.eoulsan.modules.generators.GFFFastaGeneratorModule</generator>
  <checker>fr.ens.biologie.genomique.eoulsan.checkers.GenomeChecker</checker>
  <extensions>
    <extension default="true">.fasta</extension>
    <extension>.fa</extension>
    <extension>.fna</extension>
    <extension>.fsa</extension>
  </extensions>
  <galaxy>
    <formatname>fasta</formatname>
  </galaxy>
</dataformat>
